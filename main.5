#include <iostream>
using namespace std;
int main() {
	double radius;
	const double PI = 3.14159;
	double volume;
	cout << "Vvedit radius kuli:";
	cin >> radius;
	volume = (4.0 / 3.0) * PI * radius * radius * radius;
	cout << "Obyem kuli z radiusom " << radius << " dorivnue: " << volume << endl;
	return 0;
}
