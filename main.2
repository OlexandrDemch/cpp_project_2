#include <iostream>
using namespace std;
int main() {
	int hryven, kopiyok;
	cout << "Vvedit skylko hryven:";
	cin >> hryven;
	cout << "Vvedit skylko kopiyok:";
	cin >> kopiyok;
	int excess_kopiyok = kopiyok % 100;
	hryven += kopiyok / 100;
	kopiyok = excess_kopiyok;

	cout << "Pravylna suma groshey " << hryven << " hryven " << kopiyok << " kopiyok";
	return 0;
}
