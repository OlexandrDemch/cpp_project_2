#include <iostream>
using namespace std;

int main()
{
    int year;
    cout << "Enter a year: ";
    cin >> year;

    bool isLeapYear = (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);

    if (isLeapYear) {
        cout << "In " << year << " year = 366 days" << endl;
    }
    else {
        cout << "In " << year << " year = 365 days" << endl;
    }

    return 0;
}
